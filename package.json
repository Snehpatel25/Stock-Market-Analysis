{
  "name": "stock-analysis",
  "version": "1.0.0",
  "private": true,
  "scripts": {
  "dev": "concurrently --prefix \"[{name}]\" --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\" \"npm run server\" \"npm run client\"",
  "server": "cd backend && npm run dev",
  "client": "wait-on tcp:5000 && cd frontend && npm run dev"
},
  "devDependencies": {
    "concurrently": "^8.2.2",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2"
  }
}
